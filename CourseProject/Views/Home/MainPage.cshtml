<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Users</title>
    <link href="@Url.Content("~/Content/site.css")" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/jquery-3.4.1.js"></script>
</head>
<body>
    @{ if (ViewBag.IsAdmin)
        {
            <div>
                @Html.ActionLink("List Of Users", "ListOfUsers")
            </div>
        }
        if (ViewBag.IsNotGuest)
        {
            <div>
                @Html.ActionLink("Add Collection", "AddCollection")
            </div>
        }
    }
</body>
</html>